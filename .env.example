# Earthquake Visualizer Environment Variables

# USGS API Configuration
VITE_USGS_API_BASE_URL=https://earthquake.usgs.gov/earthquakes/feed/v1.0/summary
VITE_USGS_API_TIMEOUT=10000

# Application Configuration
VITE_APP_TITLE=Earthquake Visualizer
VITE_APP_DESCRIPTION=Real-time earthquake visualization and analysis
VITE_DEFAULT_REFRESH_INTERVAL=300000
VITE_MAX_EARTHQUAKES_DISPLAY=1000

# Map Configuration
VITE_DEFAULT_MAP_CENTER_LAT=20
VITE_DEFAULT_MAP_CENTER_LNG=0
VITE_DEFAULT_MAP_ZOOM=2
VITE_MIN_MAP_ZOOM=2
VITE_MAX_MAP_ZOOM=18

# Development
VITE_ENABLE_REDUX_DEVTOOLS=true
VITE_ENABLE_DEBUG_LOGS=true